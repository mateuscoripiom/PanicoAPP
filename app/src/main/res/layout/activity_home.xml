<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/recyclerView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1E1E1E"
    tools:context=".HomeActivity">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="372dp"
        android:layout_height="423dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fundohome" />

    <Button
        android:id="@+id/button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:background="@drawable/personagem"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logopanico" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/poppins"
        android:text="@string/franquia"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView18" />

    <ImageView
        android:id="@+id/imgfundo"
        android:layout_width="407dp"
        android:layout_height="70dp"

        android:layout_marginBottom="-10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/navbar" />

    <ImageButton
        android:id="@+id/imgbtnhomeinicial"
        android:layout_width="63dp"
        android:layout_height="49dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:background="@null"
        app:layout_constraintEnd_toStartOf="@+id/imgbtnatoresinicial"
        app:layout_constraintStart_toStartOf="@+id/imgfundo"
        app:layout_constraintTop_toTopOf="@+id/imgfundo"
        app:srcCompat="@drawable/casa" />

    <ImageButton
        android:id="@+id/imgbtnatoresinicial"
        android:layout_width="63dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:background="@null"
        android:importantForAutofill="auto"
        app:layout_constraintEnd_toStartOf="@+id/imgbtnpinicial"
        app:layout_constraintTop_toTopOf="@+id/imgfundo"
        app:srcCompat="@drawable/icons8_atores_30" />

    <ImageButton
        android:id="@+id/imgbtnpinicial"
        android:layout_width="63dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:background="@null"
        android:importantForAutofill="auto"
        app:layout_constraintEnd_toStartOf="@+id/imgbtntelefoneinicial"
        app:layout_constraintTop_toTopOf="@+id/imgfundo"
        app:srcCompat="@drawable/icons8_pessoa_30" />

    <ImageButton
        android:id="@+id/imgbtntelefoneinicial"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="60dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="@+id/imgfundo"
        app:layout_constraintTop_toTopOf="@+id/imgfundo"
        app:srcCompat="@drawable/telefoneicon" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="208dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />


</androidx.constraintlayout.widget.ConstraintLayout>